{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "typeRoots": ["./src/types", "./node_modules/@types"],
    "types": ["node", "express"]
  },
  "include": ["src"]
}
